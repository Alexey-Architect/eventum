{
	"name": "eventum/eventum",
	"description": "Eventum Issue / Bug tracking system",
	"homepage": "https://launchpad.net/eventum",
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Elan RuusamÃ¤e",
			"email": "glen@delfi.ee",
			"role": "Developer"
		},
		{
			"name": "Bryan Alsdorf",
			"email": "balsdorf@gmail.com",
			"role": "Developer"
		}
	],
	"support": {
		"irc": "irc://irc.freenode.org/eventum",
		"issues": "https://bugs.launchpad.net/eventum"
	},
	"repositories": [
		{
			"type": "pear",
			"url": "http://pear.php.net",
			"vendor-alias": "pear"
		},
		{
			"type": "package",
			"package": {
				"name": "sphinx/php-sphinxapi",
				"version": "2.0.1",
				"dist": {
					"url": "http://sphinxsearch.googlecode.com/svn/tags/REL_2_0_1/api/sphinxapi.php",
					"shasum": "2b1274cbbc4b2be2aba2cb90dc5daf2036df0f49",
					"type": "file"
				},
				"autoload": {
					"classmap": [ "sphinxapi.php" ]
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "php-gettext/php-gettext",
				"version": "1.0.11",
				"dist": {
					"url": "https://launchpad.net/php-gettext/trunk/1.0.11/+download/php-gettext-1.0.11.tar.gz",
					"shasum": "9e6f402b460ba5e691e6e21f95d23527ab965f71",
					"type": "tar"
				},
				"provide":  {
					"ext-gettext":"self.version"
				},
				"autoload": {
					"classmap": [ "gettext.php", "streams.php" ]
				}
			}
		},
		{
			"type": "vcs",
			"url": "https://github.com/glensc/blockui"
		},
		{
			"type": "package",
			"package": {
				"name": "components/jquery-chosen",
				"type": "component",
				"version": "1.2.0",
				"dist": {
					"url": "https://github.com/harvesthq/chosen/releases/download/v1.2.0/chosen_v1.2.0.zip",
					"type": "zip"
				},
				"extra": {
					"component": {
						"scripts": [
							"chosen.jquery.js"
						],
						"style": [
							"chosen.css"
						],
						"files": [
							"*.png",
							"chosen.css"
						]
					}
				},
				"require": {
					"robloach/component-installer": "*"
				}
			}
		}
	],
	"config": {
		"component-dir": "htdocs/components",
		"component-baseurl": "/components"
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"require": {
		"components/jquery": "~1.8.3",
		"components/jquery-blockui": "*@dev",
		"components/jquery-chosen": "*",
		"components/jquery-cookie": "~1.4.1",
		"components/jqueryui": "*",
		"malsup/form" :"*",
		"ext-filter": "*",
		"ext-gd": "*",
		"ext-iconv": "*",
		"ext-imap": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-mysql": "*",
		"ext-pcre": "*",
		"ext-session": "*",
		"ext-spl": "*",
		"pear-pear.php.net/Auth_SASL": "*",
		"pear-pear.php.net/DB": "*",
		"pear-pear.php.net/File_Util": "*",
		"pear-pear.php.net/Mail": "*",
		"pear-pear.php.net/Mail_Mime": "*",
		"pear-pear.php.net/Mail_mimeDecode": "*",
		"pear-pear.php.net/Math_Stats": "*",
		"pear-pear.php.net/Net_LDAP2": "*",
		"pear-pear.php.net/Net_POP3": "*",
		"pear-pear.php.net/Net_SmartIRC": "*",
		"pear-pear.php.net/Net_SMTP": "*",
		"pear-pear.php.net/Net_Socket": "*",
		"pear-pear.php.net/Net_URL": "*",
		"pear-pear.php.net/PEAR": "*",
		"pear-pear.php.net/Text_Diff": "*",
		"pear-pear.php.net/XML_RPC": "*",
		"php": ">=5.1.2",
		"php-gettext/php-gettext": "1.0.*",
		"smarty-gettext/smarty-gettext": "~1.0",
		"smarty/smarty": "3.1.12",
		"sphinx/php-sphinxapi": "2.0.*"
	},
	"suggest": {
		"ext-ldap": "Adds support for LDAP auth backend"
	},
	"autoload": {
		"classmap": [ "lib/eventum/", "lib/jpgraph/"]
	}
}
